      "source": [
        "from transformers import pipeline, GPT2LMHeadModel, GPT2Tokenizer\n",
        "\n",
        "# Cargar el modelo y el tokenizador de GPT-2\n",
        "model_name = \"gpt2\"\n",
        "tokenizer = GPT2Tokenizer.from_pretrained(model_name)\n",
        "model = GPT2LMHeadModel.from_pretrained(model_name)\n",
        "\n",
        "# Crear un pipeline de generación de texto\n",
        "text_generator = pipeline(\"text-generation\", model=model, tokenizer=tokenizer)\n",
        "\n",
        "# Generar texto a partir de un prompt\n",
        "prompt = \"In a distance future, the humand and robots\"\n",
        "generated_text = text_generator(prompt, max_length=50, num_return_sequences=2, temperature=0.7)\n",
        "\n",
        "# Mostrar el texto generado\n",
        "for idx, txt in enumerate(generated_text):\n",
        "  print(f\"Texto generado {[idx]}:\\n{txt['generated_text']}\")"
      ]
    },
