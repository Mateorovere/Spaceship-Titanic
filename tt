      "cell_type": "code",
      "source": [
        "from transformers import pipeline, AutoModelForCausalLM, AutoTokenizer, Conversation\n",
        "\n",
        "# Cargar el modelo y el tokenizador\n",
        "model_name = \"microsoft/DialoGPT-medium\"\n",
        "tokenizer = AutoTokenizer.from_pretrained(model_name)\n",
        "model = AutoModelForCausalLM.from_pretrained(model_name)\n",
        "\n",
        "# Crear un pipeline de conversación\n",
        "conversation_pipeline = pipeline(\"conversational\", model=model, tokenizer=tokenizer)\n",
        "\n",
        "# Crear un objeto de conversación\n",
        "conversation = Conversation()\n",
        "\n",
        "# Agregar el mensaje inicial de la conversación\n",
        "conversation.add_user_input(\"Hello, how are you?\")\n",
        "\n",
        "# Generar una respuesta\n",
        "response = conversation_pipeline(conversation)\n",
        "\n",
        "# Agregar más mensajes a la conversación\n",
        "conversation.add_user_input(\"What can you tell me about AI?\")\n",
        "response = conversation_pipeline(conversation)\n",
        "\n",
        "# Mostrar la conversación completa\n",
        "print(conversation)"
