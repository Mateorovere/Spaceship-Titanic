        "from openai import OpenAI\n",
        "from decouple import config\n",
        "\n",
        "api_key =config('OPENAI_API_KEY')\n",
        "\n",
        "client = OpenAI(api_key=api_key)\n",
        "\n",
        "# Chain-Of-Thought example\n",
        "chain_of_thought_examples = [{\"role\": \"system\", \"content\": \"Eres un asistente inteligente que proporciona explicaciones detalladas de los procesos de pensamiento.\"},\n",
        "    {\"role\": \"user\", \"content\": \"Roger tiene 5 pelotas de tenis. Compra 2 latas más de pelotas de tenis. Cada lata tiene 3 pelotas de tenis. ¿Cuántas pelotas de tenis tiene ahora?\"},\n",
        "    {\"role\": \"assistant\",\"content\": \"Roger empezó con 5 pelotas. 2 latas de 3 pelotas de tenis cada una hacen 6 pelotas de tenis. 5 + 6 = 11. La respuesta es 11.\"},\n",
        "    {\"role\": \"user\", \"content\": \"La cafetería tenía 23 manzanas. Si usaron 20 para hacer el almuerzo y compraron 6 más, ¿cuántas manzanas tienen?\"}]\n",
        "\n",
        "\n",
        "# Requesting the model to complete the task based on the few-shot examples provided.\n",
        "completion = client.chat.completions.create(\n",
        "    model=\"gpt-3.5-turbo\",\n",
        "    messages=chain_of_thought_examples\n",
        ")\n",
        "\n",
        "# Print the completion response\n",
        "print(completion.choices[0].message.content)"
      ],
