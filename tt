        "\n",
        "api_key = config('OPENAI_API_KEY')\n",
        "\n",
        "client = OpenAI(api_key=api_key)\n",
        "\n",
        "# Few-shot examples to demonstrate the concept of recursion in programming through poetry.\n",
        "few_shot_poems = [\n",
        "    {\"role\": \"system\", \"content\": \"Eres un asistente poético, hábil en explicar conceptos complejos de programación con un toque creativo.\"},\n",
        "    {\"role\": \"user\", \"content\": \"Debes componer un poema que explique el concepto de bucles en la programación.\"},\n",
        "    {\"role\": \"assistant\", \"content\": \"Un bucle en código, un ciclo de repetir,\\nHasta que la condición ya no es tan dulce.\\nUna y otra vez, las instrucciones giramos,\\nHasta que la condición final dice, 'ya no más, ganas'.\"},\n",
        "    {\"role\": \"user\", \"content\": \"Ahora escribe un poema sobre las variables en programación.\"},\n",
        "    {\"role\": \"assistant\", \"content\": \"Las variables sostienen, con nombres tan claros,\\nUna caja para datos, de año en año.\\nContenido cambiante, destino mutable,\\nEn el agarre de la memoria, se sientan y esperan.\"},\n",
        "    # Insertando el nuevo indicativo para la recursión en programación como un poema.\n",
        "    {\"role\": \"user\", \"content\": \"Debes componer un poema que explique el concepto de recursión en la programación.\"}\n",
        "]\n",
        "\n",
        "\n",
        "# Requesting the model to complete the task based on the few-shot examples provided.\n",
        "completion = client.chat.completions.create(\n",
